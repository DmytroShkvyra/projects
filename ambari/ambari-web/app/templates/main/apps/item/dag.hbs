    <div id="dag_viewer"></div>

    <table class="table table-bordered table-striped" id="innerTable">
      <thead>
      <tr>
        <th>Job &#35;</th>
        <th>Job Id</th>
        <th>Status</th>
        <th>Maps</th>
        <th>Reduces</th>
        <th>Input</th>
        <th>Output</th>
        <th>Duration</th>
      </tr>
      </thead>
      <tbody>
      {{#each job in view.content}}
      <tr>
        <td>{{job.workflowEntityName}}</td>
        <td>{{job.id}}</td>
        <td>{{job.status}}</td>
        <td>{{job.maps}}&nbsp;({{job.mapsProgress}}%)</td>
        <td>{{job.reduces}}&nbsp;({{job.reducesProgress}}%)</td>
        <td>{{job.inputFormatted}}</td>
        <td>{{job.outputFormatted}}</td>
        <td>{{job.duration}}</td>
      </tr>
      {{/each}}
      </tbody>
    </table>


